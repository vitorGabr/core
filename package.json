    "esbuild-plugin-react18": "^0.0.7",
    "jsdom": "^24.0.0",
    "release-it": "^17.1.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "react": "^18.2.0",
    "server-only": "^0.0.1",
    "vitest": "^1.3.1"
  }
